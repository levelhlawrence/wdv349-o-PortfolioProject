{
  "extends": "../tsconfig.base.json",
  "compilerOptions": {
    "composite": true,
    "target": "es2016",
    "module": "commonjs",
    "baseUrl": "./",
    "resolveJsonModule": true,
    "outDir": "./dist",
    "rootDir": "./src",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "include": ["src"],
  "exclude": ["node_modules", "dist"],
  "references": []
}
